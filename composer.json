{
    "name": "olssonm/identity-number",
    "description": "Laravel 5 validator for Swedish personal identity numbers / social security numbers (personnummer)",
    "keywords": [
        "olssonm",
        "swedish",
        "personal identity number",
        "social security number",
        "personnummer"
    ],
    "homepage": "https://github.com/olssonm/identity-number",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Olsson",
            "email": "contact@marcusolsson.me",
            "homepage": "https://marcusolsson.me"
        }
    ],
    "require": {
        "php" : ">=5.6.0",
        "illuminate/support": "~5.3.28|~5.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.7.*",
        "orchestra/testbench": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "Olssonm\\IdentityNumber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olssonm\\IdentityNumber\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "minimum-stability": "stable"
}
